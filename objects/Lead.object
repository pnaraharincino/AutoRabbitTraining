    <fields>
        <fullName>CorrelationID__c</fullName>
        <defaultValue>&quot;none&quot;</defaultValue>
        <description>The Correlation ID Field is used by Salesforce for Google AdWords for automatic correlation of web information with new leads created through a standard web-to-lead form</description>
        <externalId>false</externalId>
        <label>CorrelationID</label>
        <length>255</length>
        <required>false</required>
        <trackFeedHistory>false</trackFeedHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Correlation_Data__c</fullName>
        <defaultValue>&quot;none&quot;</defaultValue>
        <description>The Correlation Data field is designed for use by Salesforce for Google AdWords to store all relevant lead tracking information about a web generated lead</description>
        <externalId>false</externalId>
        <label>Correlation Data</label>
        <length>32000</length>
        <trackFeedHistory>false</trackFeedHistory>
        <type>LongTextArea</type>
        <visibleLines>3</visibleLines>
    </fields>
    <fields>
        <fullName>Desired_Loan_Amount__c</fullName>
        <externalId>false</externalId>
        <label>Desired Loan Amount</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackFeedHistory>false</trackFeedHistory>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Is_this_a_Test__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>Please denote if this is a Test Record.</inlineHelpText>
        <label>Is this a Test?</label>
        <required>false</required>
        <trackFeedHistory>false</trackFeedHistory>
        <type>Picklist</type>
        <valueSet>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Yes</fullName>
                    <default>false</default>
                    <label>Yes</label>
                </value>
                <value>
                    <fullName>No</fullName>
                    <default>false</default>
                    <label>No</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>Lead_Status_Icon__c</fullName>
        <externalId>false</externalId>
        <formula>IMAGE(CASE(Status, &quot;Qualified&quot;, &quot;/img/samples/light_green.gif&quot;, &quot;Contacted&quot;, &quot;/img/samples/light_yellow.gif&quot;, &quot;Open&quot;, &quot;/img/samples/light_yellow.gif&quot;, &quot;Unqualified&quot;, &quot;/img/samples/light_red.gif&quot;, &quot;/s.gif&quot;), &quot;status color&quot;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Lead Status Icon</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Loan_Purpose__c</fullName>
        <externalId>false</externalId>
        <label>Loan Purpose</label>
        <length>32768</length>
        <trackFeedHistory>false</trackFeedHistory>
        <type>Html</type>
        <visibleLines>10</visibleLines>
    </fields>
    <fields>
        <fullName>Related_Lead__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <externalId>false</externalId>
        <inlineHelpText>This field should  not be on the layout. It is a custom field used in the custom Product Package Convert button.</inlineHelpText>
        <label>Related Lead</label>
        <referenceTo>Lead</referenceTo>
        <relationshipLabel>Leads</relationshipLabel>
        <relationshipName>Leads</relationshipName>
        <required>false</required>
        <trackFeedHistory>false</trackFeedHistory>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Web_Source__c</fullName>
        <defaultValue>&quot;none&quot;</defaultValue>
        <description>This field is designed for future use by Salesforce for Google AdWords as an alternative to the Lead Source field for web generated leads.</description>
        <externalId>false</externalId>
        <label>Web Source</label>
        <length>255</length>
        <required>false</required>
        <trackFeedHistory>false</trackFeedHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <listViews>
        <fullName>AllOpenLeads</fullName>
