    <validationRules>
        <fullName>Review_Validation_01</fullName>
        <active>true</active>
        <description>You Cannot Manually Change the Review Stage to Approval. Please Ensure That All Information is Accurate and Press the &apos;Submit for Approval&apos; Button at the Top of the Page. - RV01</description>
        <errorConditionFormula>AND(
OR(
ISNEW(),
ISCHANGED ( cm_Review_Stage__c )),
ISPICKVAL(cm_Review_Stage__c, &quot;Approval&quot;), TRUE)</errorConditionFormula>
        <errorMessage>You Cannot Manually Change the Review Stage to Approval. Please Ensure That All Information is Accurate and Press the &apos;Submit for Approval&apos; Button at the Top of the Page. - RV01</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Review_Validation_02</fullName>
        <active>true</active>
        <description>You Cannot Manually Change the Review to a Post Approval Stage. The Review Must be Approved by Pressing the &apos;Submit for Approval&apos; Button at the Top of the Page. - RV02</description>
        <errorConditionFormula>AND(
ISPICKVAL( cm_Review_Stage__c  , &apos;Complete&apos;),
ISBLANK( cm_Approved_Date__c ),
OR(
ISPICKVAL( cm_Review_Stage__c , &apos;Approval&apos;),
ISBLANK( cm_Approved_Date__c )),
$User.No_Validation__c = False)</errorConditionFormula>
        <errorMessage>You Cannot Manually Change the Review to a Post Approval Stage. The Review Must be Approved by Pressing the &apos;Submit for Approval&apos; Button at the Top of the Page. - RV02</errorMessage>
    </validationRules>
