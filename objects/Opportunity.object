    <fields>
        <fullName>cm_Conversion_Complete__c</fullName>
        <description>Used to indicate when an Opportunity has been successfully converted. Added for the Commercial Accelerate project.</description>
        <externalId>false</externalId>
        <formula>IF(NOT(ISBLANK(LLC_BI__Converted_Date__c)),
True,
False)</formula>
        <label>Conversion Complete</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>cm_Loan_Assistant__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <description>Used to capture the loan assistant for the opportunity conversion. Added for the Commercial Accelerate project.</description>
        <externalId>false</externalId>
        <label>Loan Assistant</label>
        <referenceTo>User</referenceTo>
        <relationshipName>Opportunities1</relationshipName>
        <required>false</required>
        <trackFeedHistory>false</trackFeedHistory>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>cm_Loan_Officer__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <description>Used to capture the loan officer for the opportunity conversion. Added for the Commercial Accelerate project.</description>
        <externalId>false</externalId>
        <label>Loan Officer</label>
        <referenceTo>User</referenceTo>
        <relationshipName>Opportunities</relationshipName>
        <required>false</required>
        <trackFeedHistory>false</trackFeedHistory>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>cm_Ready_to_Convert__c</fullName>
        <description>Used to render a lightning banner and Convert to Product Package action to notify users when the opportunity is ready to be converted. Added for the Commercial Accelerate project.</description>
        <externalId>false</externalId>
        <formula>IF(
AND(
NOT(ISPICKVAL(LLC_BI__Product_Line__c, &apos;&apos;)),
NOT(ISPICKVAL(LLC_BI__Product_Type__c, &apos;&apos;)),
NOT(ISPICKVAL(LLC_BI__Product__c, &apos;&apos;)),
NOT(ISBLANK(Amount)),
NOT(ISPICKVAL(Account.Type, &apos;&apos;)),
ISBLANK(LLC_BI__Converted_Date__c)
),
True,
False
)</formula>
        <label>Ready to Convert</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>isTest__c</fullName>
        <externalId>false</externalId>
        <label>isTest</label>
        <required>false</required>
        <trackFeedHistory>false</trackFeedHistory>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>No</fullName>
                    <default>true</default>
                    <label>No</label>
                </value>
                <value>
                    <fullName>Yes</fullName>
                    <default>false</default>
                    <label>Yes</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <listViews>
        <fullName>Default_Opportunity_Pipeline</fullName>
        <filterScope>Mine</filterScope>
        <filters>
            <field>OPPORTUNITY.CLOSE_DATE</field>
