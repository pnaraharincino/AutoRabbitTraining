    <webLinks>
        <fullName>Convert_to_Product_Package</fullName>
        <availability>online</availability>
        <description>Converts the source Opportunity to an nCino Product Package</description>
        <displayType>button</displayType>
        <encodingKey>UTF-8</encodingKey>
        <height>600</height>
        <linkType>url</linkType>
        <masterLabel>Convert to Product Package</masterLabel>
        <openType>sidebar</openType>
        <protected>false</protected>
        <url>/apex/LLC_BI__ComplexLifeCycleConverter?
id={!Opportunity.Id}&amp;
relationshipAPIName=Name&amp;
productApiName=Product__c&amp;
productTypeApiName=Product_Type__c&amp;
productLineApiName=Product_Line__c&amp;
mergeAccountIdApiName=AccountId&amp;
convertedDateAPIName=Converted_Date__c&amp;
convertedTargetIdAPI=Loan__c&amp;
packageReferenceAPI=Product_Package__c&amp;
targetPackageRefAPI=Product_Package__c&amp;
productPackageName={!Account.Name} - {!TODAY()}</url>
    </webLinks>
